<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package namespace="/admin" name="device" extends="comadmin">
        <action name="device*" class="deviceAction" method="{1}">
            <result name="CardShow">/WEB-INF/admin/Device/deviceCardShow.jsp</result>
            <result name="Show">/WEB-INF/admin/Device/deviceShow.jsp</result>
          
            <result name="AjaxShow" type="json">
                <param name="root">dtJson</param>
            </result>
            <result name="answer" type="json">
				<param name="root">answer</param>
			</result>
        </action>
    </package>
</struts>
